<div class="page-header">
  <h3><%= @dog.name %></h3>
</div>

<div class="row">
  <div class="col-lg-12">
    <dl class="dl-horizontal">
      <dt></dt>
      <dd><img src="<%= @dog.picture_url %>" width="250"></dd>

      <dt>Dob</dt>
      <dd><%= @dog.dob %></dd>

      <dt>Breed</dt>
      <dd><%= @dog.breed %></dd>

      <dt>Consumption (cups) per day</dt>
      <dd><%= @dog.consumption_per_day %></dd>

      <!-- <dt>Owner</dt>
      <dd><%= @dog.user_id%></dd> -->

      <dt>Food will run out on </dt>
      <dd>
        date
    </dl>




    <div class="btn-group btn-group-justified">
      <a href="/dogs" class="btn btn-info">
        Back
      </a>
      <a href="/dogs/<%= @dog.id %>/edit" class="btn btn-default">
        Edit
      </a>
      <a href="/delete_dog/<%= @dog.id %>" class="btn btn-primary" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
